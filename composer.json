{
	"name": "icybee/module-forms",
	"type": "icanboogie-module",
	"description": "Provides form management support to Icybee.",
	"keywords": [ "form" ],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://olvlvl.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/module-forms/issues",
		"source": "https://github.com/Icybee/module-forms"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": "^5.6 || ^7.0",
		"icanboogie/module-installer": "^4.0",
		"icybee/module-nodes": "^4.0"
	},
	"require-dev": {
		"icybee/module-editor": "^4.0"
	},
	"suggest": {
		"icanboogie/mailer": "A library designed to handle emails generation and sending."
	},
	"autoload": {
		"psr-4": {
			"Icybee\\Modules\\Forms\\": "lib"
		}
	}
}
